	 		   "M","N","O","P","Q","R","S","T","U","V","W","X","Y","Z","A","B","C"};
	    
	    System.out.println("Digite um texto MAIÚSCULO");
	    
	    String texto;
	    
	    boolean rep = true;
	    do {
			
	    	texto = sc.nextLine();
	    	
	    	if (texto == "0") {
				rep = false;
			} else {
				char[] div = texto.toCharArray();
				
				for (int i = 0; i < div.length; i++) {
					System.out.println(div[i]);
					for (int j = 0; j < Alfabeto.length; j++) {
						
				
							
						}
						
					}
					
				}
			}
		} while (rep);
	    
	    
	    
	    
	}
